<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.sehati.ui.home.HomeFragment"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_health"
        android:name="com.example.sehati.ui.dashboard.HealthFragment"

        tools:layout="@layout/fragment_health" />

    <fragment
        android:id="@+id/navigation_profile"
        android:name="com.example.sehati.ui.notifications.ProfileFragment"

        tools:layout="@layout/fragment_profile" />

    <fragment
        android:id="@+id/navigation_history"
        android:name="com.example.sehati.ui.history.HistoryFragment"

        tools:layout="@layout/fragment_history" />
</navigation>